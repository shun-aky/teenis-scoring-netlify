{
  "name": "tennis-scoring-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run -w @tennis/core build && npm run -w web build",
    "dev:web": "npm run -w web dev",
    "dev:mobile": "npm run -w mobile start",
    "build:core": "npm run -w @tennis/core build"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2"
  }
}

